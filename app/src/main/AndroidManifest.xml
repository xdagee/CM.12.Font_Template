<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xdagee.cm12font_template">

    <uses-feature
        android:name="org.cyanogenmod.theme"
        android:required="true" />

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />

    <meta-data
        android:name="org.cyanogenmod.theme.name"
        android:value="@string/theme_name" />
    <meta-data
        android:name="org.cyanogenmod.theme.author"
        android:value="@string/theme_author" />
    <meta-data
        android:name="org.cyanogenmod.theme.email"
        android:value="@string/theme_email" />

    <application
        android:allowBackup="false"
        android:hasCode="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/theme_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" />

</manifest>
